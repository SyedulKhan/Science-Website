    <script>
        function LoadContent(path, elementId) {
            try {
                var c = new XMLHttpRequest();
                c.open("GET", path, false);
                c.send();
                document.getElementById(elementId).innerHTML = c.response;
                document.getElementById("page_links").setAttribute("class", "Nav VNav VNavMain Dark-dark Visible");

            }
            catch (e) {
                document.getElementById("page_content").innerHTML = "<h1>" + e.message + "</h1>";
            }
        }

        function HideMenu() {
            document.getElementById("page_links").setAttribute("class", "Nav VNav VNavMain Dark-dark Invisible");
        }
    </script>
